ctions","generated":{"line":121,"column":22},"source":"components/TransactionPool.js","original":{"line":61,"column":24}},{"generated":{"line":122,"column":0},"source":"components/TransactionPool.js","original":{"line":59,"column":8}},{"generated":{"line":122,"column":42},"source":"components/TransactionPool.js","original":{"line":45,"column":6}},{"generated":{"line":122,"column":43},"source":"components/TransactionPool.js","original":{"line":44,"column":4}},{"generated":{"line":123,"column":0},"source":"components/TransactionPool.js","original":{"line":67,"column":3}},{"name":"Component","generated":{"line":127,"column":2},"source":"components/TransactionPool.js","original":{"line":9,"column":30}},{"name":"TransactionPool","generated":{"line":129,"column":15},"source":"components/TransactionPool.js","original":{"line":70,"column":15}}],"sources":{"components/TransactionPool.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\nimport { Link } from 'react-router-dom';\nimport history from '../history';\n\nconst POOL_INERVAL_MS = 10000;\n\nclass TransactionPool extends Component {\n  state = { transactionPoolMap: {} };\n\n  fetchTransactionPoolMap = () => {\n    fetch(`${document.location.origin}/api/transaction-pool-map`)\n      .then(response => response.json())\n      .then(json => this.setState({ transactionPoolMap: json }));\n  }\n\n  fetchMineTransactions = () => {\n    fetch(`${document.location.origin}/api/mine-transactions`)\n      .then(response => {\n        if (response.status ===200) {\n          alert('success');\n          history.push('./blocks');\n        } else {\n          alert('The mine-transactions block request did not complete.');\n        }\n      });\n  }\n\n  componentDidMount() {\n    this.fetchTransactionPoolMap();\n\n    this.fetchPoolMapInterval = setInterval(\n      () => this.fetchTransactionPoolMap(),\n      POOL_INERVAL_MS\n    );\n  }\n\n  componentWillUnmount() {\n    clearInterval(this.fetchPoolMapInterval);\n  }\n\n  render() {\n    return (\n      <div className='TransactionPool'>\n        <div><Link to='/'>Home</Link></div>\n        <h3>TransactionPool</h3>\n        {\n          Object.values(this.state.transactionPoolMap).map(transaction => {\n            return (\n              <div key={transaction.id}>\n                <hr />\n                <Transaction transaction={transaction} />\n              </div>\n            )\n          })\n        }\n        <hr />\n        <Button\n          bsStyle=\"danger\"\n          onClick={this.fetchMineTransactions}\n        >\n          Mine the fetchMineTransactions\n        </Button>\n      </div>\n    )\n  }\n}\n\nexport default TransactionPool;\n"},"lineCount":null}},"hash":"c98b0a0ba8454c8e0ee43cf57af16e56","cacheData":{"env":{}}}